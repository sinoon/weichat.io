extends ../layout.jade

block header
    style.
        #fix{
            position: absolute;
            width: 30px;
            height: 30px;
            border: 1px solid #000;

            top :20px;
            left: 40px;
        }

        #absolute {
            position: absolute;
            width: 30px;
            height: 30px;
            border: 1px solid #000;
            top:1px;
            left: 1px;
        }


block content
    .container
        .row
            .col-md-12
                div#fix
                    div#absolute